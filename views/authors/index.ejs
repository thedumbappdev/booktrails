<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <label for="searchAuthor">Author Name</label>
  <input
    type="text"
    name="name"
    id="searchAuthor"
    value="<%= searchOptions.name %> "
  />
  <button type="submit">Search</button>
</form>

<% authors.forEach(author => { %>
<div><%= author.name %></div>
<% }) %>
